<?xml version="1.0" encoding="UTF-8"?>

<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
	<xs:element name="parametros">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="bdoficina" minOccurs="1" maxOccurs="1"/>
				<xs:element ref="bdalmacen" minOccurs="1" maxOccurs="1"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	
	<xs:element name="bdoficina" >
		<xs:complexType>
			<xs:sequence>
				<xs:element name="ip" type="xs:string"/>
				<xs:element name="port" type="xs:string"/>
				<xs:element name="db" type="xs:string"/>
				<xs:element name="driver" type="xs:string"/>
				<xs:element name="user" type="xs:string"/>
				<xs:element name="pass" type="xs:string"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	
	<xs:element name="bdalmacen" >
		<xs:complexType>
			<xs:sequence>
				<xs:element name="ip" type="xs:string"/>
				<xs:element name="port" type="xs:string"/>
				<xs:element name="db" type="xs:string"/>
				<xs:element name="driver" type="xs:string"/>
				<xs:element name="user" type="xs:string"/>
				<xs:element name="pass" type="xs:string"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	
